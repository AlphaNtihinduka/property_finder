<section class="form-auth" text-center>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  # <img class="mb-4" src="/docs/4.3/assets/brand/bootstrap-solid.svg" alt="" width="72" height="72">
  <h1 class="h3 mb-3 font-weight-normal">Please sign up</h1>
  <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "First Name" %>
  <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "Last Name" %>
  <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "Email address", autocomplete: "email" %>

  <%= f.password_field :password, autocomplete: "new-password", placeholder: "Password", class: "form-control", id: "inputPassword" %>
  <%= f.password_field :password_confirmation, autocomplete: "new-password", placeholder: "Password confirmation", class: "form-control", id: "inputPassword" %>
  <div class="checkbox mb-3">
    <label>
      <input type="checkbox" value="remember-me"> Remember me
    </label>
  </div>
  <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
  <p class="mt-5 mb-3 text-muted">Â© 2017-2019</p>
<% end %>

<%= render "devise/shared/links" %>
</section>